generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Song {
  id        String   @id @default(uuid())
  name      String
  notes     Json
  sample    Boolean  @default(false)
  user      String?  // nếu muốn lưu user
  createdAt DateTime @default(now())
}
